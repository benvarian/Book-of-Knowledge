<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://test.org/onto.owl"
         xmlns="http://test.org/onto.owl#">

<owl:Ontology rdf:about="http://test.org/onto.owl"/>

<owl:ObjectProperty rdf:about="#has_shape">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Bacterium"/>
  <rdfs:range rdf:resource="#Shape"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_grouping">
  <rdfs:domain rdf:resource="#Bacterium"/>
  <rdfs:range rdf:resource="#Grouping"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_rare_shape">
  <rdfs:subPropertyOf rdf:resource="#has_shape"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#gram_positive">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Bacterium"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Bacterium">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Shape">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Grouping">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Rod">
  <rdfs:subClassOf rdf:resource="#Shape"/>
</owl:Class>

<owl:Class rdf:about="#Isolated">
  <rdfs:subClassOf rdf:resource="#Grouping"/>
</owl:Class>

<owl:Class rdf:about="#inPair">
  <rdfs:subClassOf rdf:resource="#Grouping"/>
</owl:Class>

<owl:NamedIndividual rdf:about="#isolated1">
  <rdf:type rdf:resource="#Isolated"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#rod1">
  <rdf:type rdf:resource="#Rod"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#my_bacterium">
  <rdf:type rdf:resource="#Bacterium"/>
  <has_shape rdf:resource="#rod1"/>
  <has_grouping rdf:resource="#isolated1"/>
  <has_grouping rdf:resource="#inpair1"/>
  <gram_positive rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</gram_positive>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#inpair1">
  <rdf:type rdf:resource="#inPair"/>
</owl:NamedIndividual>


</rdf:RDF>
